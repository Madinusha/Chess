<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="628.0" prefWidth="818.0" spacing="30.0" style="-fx-background-color: pink;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.startPageController">
   <children>
      <ImageView fitHeight="130.0" fitWidth="600.0">
         <image>
            <Image url="@../../../images/белые.png" />
         </image>
      </ImageView>
      <Button fx:id="playButton" mnemonicParsing="false" onAction="#Login" prefHeight="55.0" prefWidth="250.0" style="-fx-background-radius: 20;" text="play">
         <font>
            <Font name="Poor Richard" size="31.0" />
         </font>
      </Button>
      <Button fx:id="exitButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#exit" prefHeight="55.0" prefWidth="250.0" style="-fx-background-radius: 20;" text="exit" textAlignment="CENTER">
         <font>
            <Font name="Poor Richard" size="33.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <cursor>
            <Cursor fx:constant="CROSSHAIR" />
         </cursor>
      </Button>
      <ImageView fitHeight="130.0" fitWidth="600.0">
         <image>
            <Image url="@../../../images/черные.png" />
         </image>
      </ImageView>
   </children>
</VBox>
